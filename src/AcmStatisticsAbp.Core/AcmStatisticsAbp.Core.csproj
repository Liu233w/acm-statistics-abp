    <UserSecretsId>AbpCompanyName-AcmStatisticsAbp-56C2EF2F-ABD6-4EFC-AAF2-2E81C34E8FB1</UserSecretsId>
    <RootNamespace>AcmStatisticsAbp</RootNamespace>
  </PropertyGroup>
